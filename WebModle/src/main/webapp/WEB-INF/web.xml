<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="3.1">
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>UseraddServlet</servlet-name>
        <servlet-class>org.wstos.servlet.UseraddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UseraddServlet</servlet-name>
        <url-pattern>/UseraddServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoinServlet</servlet-name>
        <servlet-class>org.wstos.servlet.LoinServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoinServlet</servlet-name>
        <url-pattern>/LoinServlet</url-pattern>
    </servlet-mapping>
</web-app>
